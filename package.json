    "babel-jest": "^24.7.1",
    "npm-run-all": "^4.1.5",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "jest": "^24.7.1",
    "parcel-bundler": "^1.6.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-datasource-rest": "^0.7.0",
    "body-parser": "^1.19.0",
    "node-fetch": "^2.6.0"
  }
